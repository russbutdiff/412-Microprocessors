int led =6; 

void setup() {
  pinMode(led, OUTPUT);
  Serial.begin(9600);
}
void loop() {
  digitalWrite(led, HIGH);
  delay(500);
  Serial.println("ON");
  digitalWrite(led, LOW);
  delay(500); 
  Serial.println("OFF");
}
